@using OrdersGeneral.Repository.General

<!-- .block-slideshow -->
<section class="container-fluid slider">
    <div class="row">
        <div class="col-lg-8 col-md-12 col-12 ps-md-0  order-lg-1">
            <div class="owl-carousel owl-theme owl-slider">
                
                @{
                    var result = RepSliders.RepositoryMainSliders();
                    if (result.Count > 0)
                    {
                        foreach (var item in result)
                        {
                                <a class="item img-pro-banner" href="">
                                    <img src=" @item.FileNameDesktop">
                                </a>
                        }
                    }
                }
            </div>
        </div>
        <div class="col-lg-4 col-md-12 col-12 banner  order-lg-0">
            <a class="item img-pro-banner" href=" ">
                <img src="~/Helper/Layouts/images/banner1.jpg">
            </a>
            <a class="item img-pro-banner" href="#">
                <img src="~/Helper/Layouts/images/banner2.jpg">
            </a>
        </div>

    </div>
</section>