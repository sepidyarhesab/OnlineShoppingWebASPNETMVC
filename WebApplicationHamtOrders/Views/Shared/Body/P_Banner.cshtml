@using OrdersGeneral.Repository.General

<!-- .block-banner -->
<div class="row mb-5">
@{
    var result = RepBanner.RepositoryMainBanner();
    if (result.Count > 0)
    {
        foreach (var item in result)
        {
            <div class="col-md-6 mb-lg-0 mb-3">
                <div class="banner">
                    <a href="#">
                        <img src="@item.FileNameDesktop" alt="@item.PrimaryTitle">
                    </a>
                </div>
            </div>
        }
    }
}
    
</div>