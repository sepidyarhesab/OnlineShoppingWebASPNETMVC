@using OrdersOrders.Repository.Orders
@using OrdersGeneral.Repository.General


<div class="row mb-5">
    <div class="col-lg-3 col-md-6 col-sm-6 mb-lg-0 mb-3">
        <div class="banner">
            @{
                var resultts = RepBanner.RepositoryBannerDashboardDown();
                if (resultts.Count > 0)
                {
                    foreach (var item in resultts)
                    {
                        <a href="@item.Url">
                            <img src="@item.FileNameDesktop" alt="@item.PrimaryTitle">
                        </a>
                    }
                }
            }
        </div>
    </div>
</div>