
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <!-- css -->
    <link rel="stylesheet" href="/Helper/Layouts/vendor/bootstrap-4.2.1/css/bootstrap.min.css">
    <link rel="stylesheet" href="/Helper/Layouts/vendor/owl-carousel-2.3.4/assets/owl.carousel.min.css">
    <link rel="stylesheet" href="/Helper/Layouts/vendor/lightgallery-1.6.12/css/lightgallery.min.css">
    <link rel="stylesheet" href="/Helper/Layouts/css/style.css">
    <link rel="stylesheet" href="/Helper/Layouts/css/custom.css">
    <link href="~/Helper/Layouts/css/colors.css" rel="stylesheet" />
    <link href="~/Helper/Admin/Content/panel.css" rel="stylesheet" />
    <!-- js -->
    <script src="/Helper/Layouts/vendor/jquery-3.3.1/jquery.min.js"></script>
    <script src="/Helper/Layouts/vendor/bootstrap-4.2.1/js/bootstrap.bundle.min.js"></script>
    <script src="/Helper/Layouts/vendor/owl-carousel-2.3.4/owl.carousel.min.js"></script>
    <script src="/Helper/Layouts/vendor/nouislider-12.1.0/nouislider.min.js"></script>
    <script src="/Helper/Layouts/vendor/lightgallery-1.6.12/js/lightgallery.min.js"></script>
    <script src="/Helper/Layouts/vendor/lightgallery-1.6.12/js/lg-thumbnail.min.js"></script>
    <script src="/Helper/Layouts/vendor/lightgallery-1.6.12/js/lg-zoom.min.js"></script>
    <script src="/Helper/Layouts/js/number.js"></script>
    <script src="/Helper/Layouts/js/main.js"></script>
    <script src="/Helper/Layouts/vendor/svg4everybody-2.1.9/svg4everybody.min.js"></script>
    <script>
        svg4everybody();
    </script>
    <!-- font - fontawesome -->
    <link rel="stylesheet" href="/Helper/Layouts/vendor/fontawesome-5.6.3/css/all.min.css">
    <!-- font - stroyka -->
    <link rel="stylesheet" href="/Helper/Layouts/fonts/stroyka/stroyka.css">



</head>
<body>
    <!-- site__body -->
    <div class="site__body">
        <div class="block">
            <div class="container">
                <div class="not-found">
                    @{

                        if (Session["TokenMessage"] != null)
                        {
                            <div class="not-found__404"> خطا!</div>
                            <div class="not-found__content">
                                <h2 class="not-found__title">@Session["TokenMessage"].ToString()</h2>
                                <p> </p>

                                <p class="not-found__text">لطفا جهت تکمیل اطلاعات سایت خود وارد لینک زیر شوید.</p><a class="btn btn-secondary btn-sm" href="/Installer/Index">رفتن به صفحه ثبت اصلاعات سایت</a>
                            </div>
                        }
                        else
                        {
                            <div class="not-found__404"> خطا!</div>
                            <div class="not-found__content">
                                <h2 class="not-found__title">اطلاعات شما ناقص است</h2>
                                <p> </p>

                                <p class="not-found__text">لطفا جهت تکمیل اطلاعات سایت خود وارد لینک زیر شوید.</p><a class="btn btn-secondary btn-sm" href="/Installer/Index">رفتن به صفحه ثبت اصلاعات سایت</a>
                            </div>
                        }




                    }

                </div>
            </div>
        </div>
    </div>
    <!-- site__body / end -->
</body>
</html>
